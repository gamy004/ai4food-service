{
  "name": "ai4food-service",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "dev": "nest start --watch",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs",
    "schema:sync": "yarn build && typeorm-ts-node-commonjs schema:sync -d dist/common/datasource/typeorm.datasource.js",
    "migration:generate": "yarn build && typeorm-ts-node-commonjs migration:generate -d dist/common/datasource/typeorm.datasource.js",
    "migration:run": "yarn build && typeorm-ts-node-commonjs migration:run -d dist/common/datasource/typeorm.datasource.js",
    "migration:revert": "yarn build && typeorm-ts-node-commonjs migration:revert -d dist/common/datasource/typeorm.datasource.js",
    "seed": "yarn build && ts-node ./dist/common/datasource/seeder.datasource.js",
    "jwt:secret": "node -e \"console.log(require('crypto').randomBytes(256).toString('base64'));\""
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.192.0",
    "@aws-sdk/s3-presigned-post": "^3.192.0",
    "@aws-sdk/util-endpoints": "^3.192.0",
    "@nestjs/common": "^9.1.4",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.1.4",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/mapped-types": "^1.2.0",
    "@nestjs/microservices": "^9.1.4",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.1.4",
    "@nestjs/typeorm": "^9.0.1",
    "aws-sdk": "^2.1236.0",
    "bcrypt": "^5.1.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^1.3.7",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-basic-auth": "^1.2.1",
    "lodash": "^4.17.21",
    "mqtt": "^4.3.7",
    "mysql2": "^2.3.3",
    "nest-aws-sdk": "^3.0.0",
    "nestjs-typeorm-paginate": "^4.0.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.7",
    "typeorm": "^0.3.10",
    "typeorm-extension": "^2.1.15",
    "typeorm-naming-strategies": "^4.1.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.4",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/swagger": "^6.1.2",
    "@nestjs/testing": "^9.1.4",
    "@types/express": "^4.17.14",
    "@types/jest": "29.2.0",
    "@types/node": "^18.11.2",
    "@types/passport-jwt": "^3.0.7",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "29.2.1",
    "prettier": "^2.7.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.0",
    "swagger-ui-express": "^4.5.0",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^~/(.*)$": "<rootDir>/$1"
    }
  }
}
